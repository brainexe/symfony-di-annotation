{
  "name": "christianblos/symfony-di-annotation",
  "description": "Allows you to configure symfony dependency injections via annotations",
  "license": "MIT",
  "require": {
    "php": "~7.3",
    "doctrine/annotations": "~1.0",
    "symfony/dependency-injection": ">=4.0",
    "symfony/config": ">=4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Symfony\\Component\\DependencyInjection\\Annotation\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "examples/",
      "tests/cases/"
    ]
  }
}
